#include <iostream>
using namespace std;
int main(){
  try{
  int donuts,milk,dpg;
  cout<<"Enter number of donuts   ";
  cin>>donuts;
  cout<<"Enter number of glasses of milk   ";
  cin>>milk;
  if(milk<=0)
    throw donuts;
  dpg=donuts/milk;
  cout<<"dpg is "<<dpg<<endl;
  }
  catch(int e){
    cout<<"donuts"<<e<<endl;
  }
  return 0;

}
